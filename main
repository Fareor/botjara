import discord
import time
from keras.models import load_model  # TensorFlow is required for Keras to work
from PIL import Image, ImageOps  # Install pillow instead of PIL
import numpy as np
from model import get_class
from discord.ext import commands
import asyncio



intents = discord.Intents.all()
intents.message_content = True
bot = commands.Bot(command_prefix='!', intents=intents)
@bot.command()
async def save(ctx):
    image = ctx.message.attachments
    if ctx.message.attachments:
      for image in image:
        file_name = image.filename
        file_url = image.url
        await image.save(f'./images/{file_name}')
        await ctx.send('картинка сохранена')
        if get_class(f"./{image.filename}") == 'Нейронка':
            await ctx.send('Похоже ваше фото кота делано нейросеткой')

            await ctx.send('и он не реальный')

        if get_class(f"./{image.filename}") == ' Сиамская кошка':
            await ctx.send("Сиамская кошка - это порода кошек с уникальным окрасом и особенностями.")

            await ctx.send("Они имеют длинное, стройное тело и глубоко синие глаза.")

            await ctx.send("Характеризуются высокой активностью, любознательностью и социальностью.")

            await ctx.send("У сиамских кошек есть особенность - они могут быть очень разговорчивыми и издают много звуков.")

            await ctx.send("Они также известны своей привязанностью к своим владельцам и склонностью к общению.")

            await ctx.send("Кроме того, сиамские кошки могут быть непредсказуемыми и иметь свои уникальные привычки.")

        if get_class(f"./{image.filename}") == 'Немецкий рекс':
            await ctx.send("Немецкий рекс - это порода кошек с кудрявой шерстью и изящными чертами лица.")

            await ctx.send("Они имеют среднего размера тело и большие выразительные глаза.")

            await ctx.send("Характеризуются спокойствием, нежностью и преданностью своим владельцам.")

            await ctx.send("У немецких рексов также есть особенность - их шерсть не линяет и не вызывает аллергию.")

            await ctx.send("Они известны своей игривостью и любовью к активным играм.")

            await ctx.send("Кроме того, немецкие рексы могут быть очень ласковыми и общительными.")

            
        if get_class(f"./{image.filename}") == 'Канадский сфинкс':
            await ctx.send("Канадский сфинкс - это порода кошек без шерсти с мускулистым телом и большими ушами.")

            await ctx.send("Они имеют гладкую и теплую кожу, которая придает им уникальный вид.")

            await ctx.send("Характеризуются энергичностью, игривостью и социальностью.")

            await ctx.send("У канадских сфинксов также есть особенность - они могут быть очень теплыми и любят обниматься.")

            await ctx.send("Они известны своей активностью и любопытством.")

            await ctx.send("Кроме того, канадские сфинксы могут быть очень лояльными и преданными.")

            
        if get_class(f"./{image.filename}") == 'Ориентальная кошка':
            await ctx.send("Ориентальная кошка - это порода кошек с длинным телом и большими ушами.")

            await ctx.send("Они имеют выразительные глаза и грациозную осанку.")
            
            await ctx.send("Характеризуются энергичностью, любознательностью и коммуникабельностью.")

            await ctx.send("У ориентальных кошек также есть особенность - они могут быть очень разговорчивыми и мурлыкают часто.")

            await ctx.send("Они известны своей привязанностью к своим владельцам и склонностью к играм.")

            await ctx.send("Кроме того, ориентальные кошки могут быть очень дружелюбными и общительными.")

            
        if get_class(f"./{image.filename}") == 'Сиамская кошка':
            await ctx.send("Бенгальская кошка - это порода кошек с уникальным окрасом, напоминающим леопарда.")

            await ctx.send("Они имеют мускулистое тело и яркие выразительные глаза.")

            await ctx.send("Характеризуются энергичностью, игривостью и любознательностью.")

            await ctx.send("У бенгальских кошек также есть особенность - они могут быть очень активными и любят лазить по высоким местам.")

            await ctx.send("Они известны своей преданностью своим владельцам и любят проводить время с ними.")

            await ctx.send("Кроме того, бенгальские кошки могут быть очень умными и обучаемыми.")

            
        if get_class(f"./{image.filename}") == 'Сибирская кошка':
            await ctx.send("Сибирская кошка - порода кошек сильной и мускулистой конструкции.")

            await ctx.send("Они обладают густой двойной шерстью, которая защищает их от холода.")
            await ctx.send("Сибирские кошки дружелюбны, но независимы.")

            await ctx.send("Они известны своей игривостью и спокойным характером.")

            await ctx.send("Особенностью сибирских кошек является их способность производить аллергены в меньшем количестве, что делает их подходящими для людей с аллергией.")

            
        if get_class(f"./{image.filename}") == 'Русская голубая кошка':
            await ctx.send("Русская голубая кошка - порода кошек с красивым серебристо-синим окрасом.")

            await ctx.send("Они имеют густую, пушистую шерсть и яркие зеленые глаза.")

            await ctx.send("Русские голубые кошки обладают нежным и ласковым характером.")

            await ctx.send("Они очень преданные и легко привязываются к своим хозяевам.")

            await ctx.send("Эта порода известна своей умностью и игривостью.")

        
        if get_class(f"./{image.filename}") == 'Американский керл':
            await ctx.send("Американский керл - порода кошек с уникальными изогнутыми ушами.")

